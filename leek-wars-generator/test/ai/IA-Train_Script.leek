function tryAndUseChip(chip, leek) {
	if (canUseChip(chip, leek)) {
		useChip(chip,leek);
		return true;
	}
	return false;
}

function setAndUseWeapon_(weapon, enemy, doMax) {
	if (canUseWeapon(weapon, enemy)) {
		if (getWeapon() != weapon) {
			setWeapon(weapon);
		}// On essaye de lui tirer dessus !
		useWeapon(enemy);
		var lastTP = getTP();
		while (doMax && lastTP >= getWeaponCost()) {
		  useWeapon(enemy);
		  if (getTP() == lastTP) {
		  	break;
		  }
		  lastTP = getTP();
		}
		return true;
	}
}