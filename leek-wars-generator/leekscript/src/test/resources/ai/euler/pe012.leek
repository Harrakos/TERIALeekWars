var n = 50000
var N = 500
var d = []
fill(d, 0, n + 2)

for (var i = 1; i <= n + 1; i++) {
	for (var j = i; j <= n + 1; j += i) {
		d[j]++
	}
}

var r = 0
var t = 0
for (var i = 1; i <= n; i++) {
	var res = 0
	if (i & 1) {
		res = d[i] * d[(i + 1) / 2]
	} else {
		res = d[i / 2] * d[i + 1]
	}
	if (res > t) {
		t = res
	}
	if (res > N) {
		r = i * (i + 1) / 2 | 0
		break
	}
}
return r;
